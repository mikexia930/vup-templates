{
  "buildCommand": "pnpm build",
  "outputDirectory": ".output",
  "framework": null,
  "installCommand": "pnpm install --frozen-lockfile",
  "devCommand": "pnpm dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
